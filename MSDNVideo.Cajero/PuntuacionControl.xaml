<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2006" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d"
    x:Class="MSDNVideo.Cajero.PuntuacionControl"
    Height="21" Width="85"
    Loaded="UserControl_Loaded">
	<Grid HorizontalAlignment="Left" Width="Auto" Height="Auto">
	
		<StackPanel Orientation="Horizontal" x:Name="backPanel" d:LayoutOverrides="Width" Opacity="0.2">
			<Image Width="17" Height="17" Source="Images\IconoPreferidas.png"/>
			<Image Width="17" Height="17" Source="Images\IconoPreferidas.png"/>
			<Image Width="17" Height="17" Source="Images\IconoPreferidas.png"/>
			<Image Width="17" Height="17" Source="Images\IconoPreferidas.png"/>
			<Image Width="17" Height="17" Source="Images\IconoPreferidas.png"/>
		</StackPanel>
		
		<StackPanel Orientation="Horizontal" x:Name="mainPanel" d:LayoutOverrides="Width" HorizontalAlignment="Left">
			<Image Width="17" Height="17" Source="Images\IconoPreferidas.png"/>
			<Image Width="17" Height="17" Source="Images\IconoPreferidas.png"/>
			<Image Width="17" Height="17" Source="Images\IconoPreferidas.png"/>
			<Image Width="17" Height="17" Source="Images\IconoPreferidas.png"/>
			<Image Width="17" Height="17" Source="Images\IconoPreferidas.png"/>
		</StackPanel>
		
	</Grid>
</UserControl>
